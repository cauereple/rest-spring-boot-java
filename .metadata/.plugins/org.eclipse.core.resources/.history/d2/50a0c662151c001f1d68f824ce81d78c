public class HeroMapperImpl extends HeroMapper {

    @Override
    public Hero toHero(SaveHero saveHero) {
        if ( saveHero == null ) {
            return null;
        }

        Hero hero = new Hero();

        hero.setAbility( saveHero.getAbility() );
        hero.setDefense( saveHero.getDefense() );
        hero.setHealth( saveHero.getHealth() );
        hero.setName( saveHero.getName() );
        hero.setType( saveHero.getType() );

        return hero;
    }

    @Override
    public Hero toHero(UpdateHero updateHero) {
        if ( updateHero == null ) {
            return null;
        }

        Hero hero = new Hero();

        hero.setAbility( updateHero.getAbility() );
        hero.setDefense( updateHero.getDefense() );
        hero.setHealth( updateHero.getHealth() );
        hero.setId( updateHero.getId() );
        hero.setName( updateHero.getName() );
        hero.setType( updateHero.getType() );

        return hero;
